int false()  {return 0;}
int true() {return 1;}
void main() {
	int a,b,c;

	a = true() || true(); @a;@" ";
	a = false() || true(); @a;@" ";
	a = true() || false(); @a;@" ";
	a = false() || false(); @a;@" ";

	a = true() && true(); @a;@" ";
	a = false() && true(); @a;@" ";
	a = true() && false(); @a;@" ";
	a = false() && false(); @a;@" ";

	a = !false(); @a;@" ";
	a = !true(); @a;@" ";

	@"dbg";
	@!(true() == false() && true() == 0); @" "; 
	@ true() == false() && true() == 0; @" "; 
	@ true() == false(); @" ";
	@ true() == 0; @" ";
	@ (false() == true()) && false(); @" "; 
	@"Combining";
	b=1;c=1;a=1;
	a = !(a == b && c == 0); @a;@" "; 
	b=0;c=1;a=1;
	a = !(a == b && c == 0); @a;@" "; /* should be true */
	b=1;c=0;a=1;
	a = !(a == b && c == 0); @a;@" "; /* false*/
	b=0;c=0;a=1;
	a = !(a == b && c == 0); @a;@" ";  /* should be true*/

	b=1;c=1;a=0;
	a = !(a == b && c == 0); @a;@" ";
	b=0;c=1;a=0;
	a = !(a == b && c == 0); @a;@" ";
	b=1;c=0;a=0;
	a = !(a == b && c == 0); @a;@" ";
	b=0;c=0;a=0;
	a = !(a == b && c == 0); @a;@" ";


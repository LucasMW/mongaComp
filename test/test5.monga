char[][] vector;
int f(char[] a) {
	
	return a[0] as int;
}
int main (int argc,char[] argv)
{
	int a;
	int b;
	vector = new char[][10];
	@"after new";
	a = 0; b= 10;
	while(a<b) {
		@"before while new";
		vector[a] = new char[b];
		@"after new";
		vector[a][0] = (2 as char);
		@"after as char";
		a=a+1;
	}
